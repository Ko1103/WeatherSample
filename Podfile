	pod 'Alamofire', '~> 3.0'
	pod 'RxAlamofire'

  target 'WeatherSampleTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WeatherSampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
